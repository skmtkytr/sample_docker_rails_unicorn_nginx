doctype html
html
  head
    meta[http-equiv="X-UA-Compatible" content="IE=edge"]
    meta[name="viewport" content="width=device-width, initial-scale=1"]
    title
      | eFriends
    = csrf_meta_tags
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'
  body
    nav.navbar.navbar-inverse.navbar-fixed-top
      .container
        .navbar-header
        #navbar.navbar-collapse.collapse
          button.navbar-toggle.collapsed aria-controls="navbar" aria-expanded="false" data-target="#navbar" data-toggle="collapse" type="button"
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand href="/" THIS SITE

          - if user_signed_in?
            ul.nav.navbar-nav
              li.dropdown.active
                strong
                a.dropdown-toggle aria-expanded="false" aria-haspopup="true" data-toggle="dropdown" href="#" role="button"
                  = current_user.username
                  span.badge 0
                  span.caret
                ul.dropdown-menu
                  li
                    = link_to current_user.username, pages_show_path
                  li
                    = link_to 'プロフィール変更', edit_user_registration_path
                  li
                    = link_to 'ログアウト', destroy_user_session_path, method: :delete
                  li
                  li.divider role="separator"
                    li.dropdown-header Nav header
                li
                  = link_to 'wsChat', '/application/home'
                li
          - else
            ul.nav.navbar-nav
              li.dropdown.active
                strong
                a.dropdown-toggle aria-expanded="false" aria-haspopup="true" data-toggle="dropdown" href="" role="button"
                  = 'Home'
                  span.caret
                ul.dropdown-menu
                  li
                    = link_to 'サインアップ', new_user_registration_path
                  li
                    = link_to 'ログイン', new_user_session_path
                  li
                  li.divider role="separator"
                    li.dropdown-header Nav header
                li
                  = link_to 'wsChat', '/application/home'
                  /= link_to 'wsChat', {:controller => 'application', :action => 'home'}
                li
            /! /.nav-collapse
            ul.nav.navbar-nav.navbar-right
              li
                a Default
              li
                a Static top
          /#notice
          /  .alert.alert-info
          /    = alert
          /    = notice
    .container
      #carousel-generic.carousel.slide data-ride="carousel"
        ol.carousel-indicators
          li data-slide-to="0" data-target="#carousel-generic"
          li data-slide-to="1" data-target="#carousel-generic"
          li.active data-slide-to="2" data-target="#carousel-generic"
        .carousel-inner role="listbox"
          .item
            /img alt=("First slide [1140x160]") height=160 data-holder-rendered="true" data-src=("holder.js/1140x160/auto/#777:#555/text:First slide") src="data:image/svg+xml;base64," /
            img alt=("First slide [1140x160]") height=160 data-holder-rendered="true" data-src=("holder.js/1140x160/auto/#777:#555/text:First slide") src="data:image/svg+xml;base64," /
          .item
            img alt=("First slide [1140x160]") height=160 data-holder-rendered="true" data-src=("holder.js/1140x160/auto/#777:#555/text:First slide") src="data:image/svg+xml;base64," /
            /img alt=("Second slide [1140x160]") height=160 data-holder-rendered="true" data-src=("holder.js/1140x160/auto/#666:#444/text:Second slide") src="data:image/svg+xml;base64," /
          .item.active
            img alt=("First slide [1140x160]") height=160 data-holder-rendered="true" data-src=("holder.js/1140x160/auto/#777:#555/text:First slide") src="data:image/svg+xml;base64," /
            /img alt=("Third slide [1140x160]") height=160 data-holder-rendered="true" data-src=("holder.js/1140x160/auto/#555:#333/text:Third slide") src="data:image/svg+xml;base64," /
        a.left.carousel-control data-slide="prev" href="#carousel-generic" role="button"
          span.glyphicon.glyphicon-chevron-left aria-hidden="true"
          span.sr-only Previous
        a.right.carousel-control data-slide="next" href="#carousel-generic" role="button"
          span.glyphicon.glyphicon-chevron-right aria-hidden="true"
          span.sr-only Next
      .jumbotron
        = yield
